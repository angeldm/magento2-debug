<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <Angeldm_Debug>
            <general>
                <active>0</active>
                <active_adminhtml>0</active_adminhtml>
                <allowed_ips>127.0.0.1</allowed_ips>
                <error_handler>whoops</error_handler>
            </general>
            <time>
                <precision>2</precision>
            </time>
            <collector>
                <ajax>1</ajax>
                <cache>1</cache>
                <config>1</config>
                <customer>1</customer>
                <database>1</database>
                <event>1</event>
                <plugin>1</plugin>
                <layout>1</layout>
                <memory>1</memory>
                <model>1</model>
                <time>1</time>
                <translation>1</translation>
            </collector>
            <performance>
                <core_color>#FF5722</core_color>
                <routing_color>#009688</routing_color>
                <config_color>#795548</config_color>
                <event_color>#2196F3</event_color>
                <layout_color>#4CAF50</layout_color>
                <eav_color>#3F51B5</eav_color>
                <controller_color>#9C27B0</controller_color>
                <template_color>#607D8B</template_color>
                <debug_color>#000000</debug_color>
                <unknown_color>#9E9E9E</unknown_color>
            </performance>
            <profiler>
                <collectors>
                    <config>Angeldm\Debug\Model\Collector\ConfigCollector</config>
                    <request>Angeldm\Debug\Model\Collector\RequestCollector</request>
                    <memory>Angeldm\Debug\Model\Collector\MemoryCollector</memory>
                    <time>Angeldm\Debug\Model\Collector\TimeCollector</time>
                    <ajax>Angeldm\Debug\Model\Collector\AjaxCollector</ajax>
                    <database>Angeldm\Debug\Model\Collector\DatabaseCollector</database>
                    <translation>Angeldm\Debug\Model\Collector\TranslationCollector</translation>
                    <customer>Angeldm\Debug\Model\Collector\CustomerCollector</customer>
                    <layout>Angeldm\Debug\Model\Collector\LayoutCollector</layout>
                    <cache>Angeldm\Debug\Model\Collector\CacheCollector</cache>
                    <model>Angeldm\Debug\Model\Collector\ModelCollector</model>
                    <event>Angeldm\Debug\Model\Collector\EventCollector</event>
                    <plugin>Angeldm\Debug\Model\Collector\PluginCollector</plugin>
                </collectors>
            </profiler>
        </Angeldm_Debug>
    </default>
</config>
